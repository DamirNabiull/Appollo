<Window x:Class="W2021.WasNotAwarded.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:W2021.WasNotAwarded"
        mc:Ignorable="d"
        Background="Black"
        Loaded="Window_Loaded"
        WindowState="Maximized"
        WindowStyle="None"
        Title="MainWindow" Height="1920" Width="1080">
    <Grid>
        <Viewbox Grid.Row="1" Grid.Column="1" Stretch="Uniform">
            <Frame x:Name="NavFrame" Width="1080px" Height="1920px" NavigationUIVisibility="Hidden"></Frame>
        </Viewbox>
    </Grid>
</Window>

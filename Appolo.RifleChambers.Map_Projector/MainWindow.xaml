<Window x:Class="Appolo.RifleChambers.Map_Projector.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Appolo.RifleChambers.Map_Projector"
        mc:Ignorable="d"
        Loaded="Window_Loaded"
        WindowStyle="None"
        Title="MainWindow" 
        Closing="Window_Closing"
        Height="1080" Width="1920">
    <Grid Height="1080" Width="1920">
        <Grid Height="1080" Width="1920">
            <Grid.Background>
                <ImageBrush x:Name="Photo" ImageSource="images/default.png"/>
            </Grid.Background>
            <Image x:Name="New_Photo"  Height="1080" Width="1920" Stretch="Fill"></Image>
        </Grid>
    </Grid>
</Window>

<Page x:Class="Appolo.RifleChambers.Map_Projector.MapPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Appolo.RifleChambers.Map_Projector"
      mc:Ignorable="d" 
      Title="MapPage"
      d:DesignHeight="1080" d:DesignWidth="1920">
    <Grid Height="1080" Width="1920" Background="#000000">
        <Grid Height="1080" Width="1920">
            <Grid.Background>
                <ImageBrush x:Name="Photo"/>
            </Grid.Background>
            <Image x:Name="New_Photo"  Height="1080" Width="1920" Stretch="Fill"></Image>
        </Grid>
    </Grid>
</Page>

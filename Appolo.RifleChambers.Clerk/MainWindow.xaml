<Window x:Class="Appolo.RifleChambers.Clerk.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Appolo.RifleChambers.Clerk"
        mc:Ignorable="d"
        Background="Black"
        Loaded="Window_Loaded"
        Closing="Window_Closing"
        WindowState="Maximized"
        WindowStyle="None"
        Title="MainWindow" Height="1080" Width="1920">
    <Grid>
        <Viewbox Grid.Row="1" Grid.Column="1" Stretch="Uniform">
            <Frame x:Name="NavFrame" Width="1920px" Height="1080px" NavigationUIVisibility="Hidden"></Frame>
        </Viewbox>
    </Grid>
</Window>
